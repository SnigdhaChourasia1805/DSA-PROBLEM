#include<bits/stdc++.h>
using namespace std;
void printArray(int arr[],int n){
    cout<<"Printing the array"<<endl;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" "<<endl;
    }
}
int main(){
    
    int arr[4]={1,3,5,7};
    cout<<"The value at index 4:"<<arr[2]<<endl;
    char ch[5]={'a','e','i','o','u'};
    cout<<"Element at index 3:"<<arr[3]<<endl;
    cout<< "Printing the array " << endl;
    for(int i =0; i<5; i++) {
        cout << ch[i] << " ";
    }
    return 0;
}
 
